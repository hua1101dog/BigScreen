<div class="modal-header">
    <h4 class="modal-title">{{item._id?'编辑':'添加'}}接口前缀</h4>
</div>
<div class="modal-body container-fluid form-horizontal">
    <form name="form" novalidate>
    <div class="form-group">
        <label class="col-xs-4 control-label required">名称：</label>
        <div class="col-xs-6">
            <input type="text" class="form-control" ng-model="item.name" name="name" required/>
        </div>
    </div>
    <div class="form-group">
        <label class="col-xs-4 control-label ">前缀：</label>
        <div class="col-xs-6">
            <input type="text" class="form-control" ng-model="item.prefix" name="prefix" />
        </div>
    </div>
    </form>
</div>
<div class="modal-footer">
    <button class="btn btn-primary" ng-click="save(form,item)">确定</button>
    <button class="btn btn-warning" ng-click="$dismiss()">取消</button>
</div>

