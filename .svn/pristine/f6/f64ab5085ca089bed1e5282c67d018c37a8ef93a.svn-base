thead.title th,tr th{
    text-align: center;
    white-space: nowrap;
}
.btn-link{
    cursor: pointer;
}
.layui-layer-iframe{
    overflow-y: hidden!important;
}
.color-block{
    display:inline-block;width: 45px ;height: 20px;box-shadow:2px 4px 3px 2px rgba(0,0,0,.2);vertical-align: text-bottom;
}
.table>tbody>tr.narrowTd>td{
    padding:8px 3px;
}
.table-striped>thead>tr>th{
    position: relative;
}
.tooltip{
    width: max-content!important;
}
.table-striped>tbody>tr.bg-success{
    background-color: #dff0d8;
}
.popoverMax{
    max-width: none;
    width: max-content;
