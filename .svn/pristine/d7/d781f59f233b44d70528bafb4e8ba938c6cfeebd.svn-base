    <meta charset="UTF-8">
	    <meta name="content-type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0,maximum-scale=1.0,minimum-scale=1.0,user-scalable=no,shrink-to-fit=no">
    <meta name="MobileOptimized" content="320">
    <meta http-equiv="Cache-Control" content="no-cache, must-revalidate">
    <meta http-equiv="Pragma" content="no-cache">
    <meta name="HandheldFriendly" content="true">
    <meta name="x5-page-mode" content="no-title">
    <title>智慧园区服务部署清单</title>

    <link href="/res/bootstrap-3.3.5/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
    <div id="app">
        <table class="table table-bordered table-striped table-hover">
            <caption>智慧园区服务部署清单</caption>
            <thead>
            <tr>
                <th rowspan="2">环境</th>  <th rowspan="2">访问主页</th>   <th rowspan="2">服务器ip</th>
                <th colspan="3">详情</th>
            </tr>
            <tr> <th>应用</th><th> 路径</th>   <th>端口</th> </tr>
            </thead>
            <tfoot><tr><td class="text-right" colspan="6">更新时间：2019-03-05 16:50</td></tr>
            </tfoot>
            <tbody>
                <template v-for="item in serverList">
                    <tr >
                        <td v-bind:rowspan="item.details.length+1">{{item.name}}</td>
                        <td v-bind:rowspan="item.details.length+1">{{item.url}}</td>
                        <td v-bind:rowspan="item.details.length+1">{{item.ip}}</td>
                    </tr>
                    <tr v-for="sub in item.details">
                        <td>
                            {{sub.app}}
                        </td>
                        <td>
                            {{sub.path}}
                        </td>
                        <td>
                            {{sub.port}}
                        </td>
                    </tr>
                </template>
            </tbody>
        </table>
    </div>
</body>
