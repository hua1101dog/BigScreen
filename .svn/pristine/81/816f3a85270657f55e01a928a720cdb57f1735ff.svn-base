@charset "utf-8";
/* CSS Document */
/*
 *谷歌滚动条
 */
*::-webkit-scrollbar {background:#f0f1ef; overflow:visible; width:9px; height: 9px;}
*::-webkit-scrollbar-corner {background:transparent;}
*::-webkit-scrollbar-thumb {background-color: #999; border-radius: 8px;}
*::-webkit-scrollbar-thumb:hover {background-color: #666;}
.ng-submitted .ng-invalid,.ng-dirty.ng-invalid:not(form):not([ng-form]),.not-valid input{
    border:1px solid rgba(255, 0, 0, 0.59);
    -moz-box-shadow:0 0 8px rgba(255, 0, 0, 0.59);
    -webkit-box-shadow:0 0 8px rgba(255, 0, 0, 0.59);
    box-shadow:0 0 8px rgba(255, 0, 0, 0.59);
}
input.waiting{background:url(/res/img/waiting.gif) right no-repeat;}
.form-inline .form-group+.form-group{
    margin-left:5px ;
}
.form-inline .form-group label{
    line-height: 36px;
    margin-bottom: 0;
}

.modal-max{
    width:90%;
}
@media (min-width: 768px)  and (max-width: 991px){
    .modal-dialog {
        width: 100%;
        margin: 30px auto;
    }
    .modal-max{
        width:100%;
    }
}


table.table tr th.text-center:first-child{
    width:20px;
    white-space: nowrap;
}

table.table tr th:last-child{
    white-space: nowrap;
}
.panel-heading button,.panel-heading .btn-group{margin-top: -7px;}
.mt7{margin-top:7px; }
.mr15{margin-right:15px; }
.mr5{margin-right:15px; }
label{white-space: nowrap}
label.required:before {content:"*";color: #ff0000;vertical-align: middle;}

table.table i.glyphicon-check{ color:#3c763d }

/*
自定义星星样式
*/
.star-on{ background: url("/res/img/star-on.png"); width: 24px; height: 24px; }
.star-off{ background: url("/res/img/star-off.png"); width: 24px; height: 24px; }
/*
上传图片的
*/
.photoWraper{ width: 90px; position: relative; float: left; margin-right: 5px;text-align: center;	}
.photoWraper>div:hover{background-color: #F5F5F5;cursor:zoom-in }
.photoWraper>div:first-child{width: 70px;height: 70px;margin:0 auto;text-align: center;line-height: 70px; border: 1px solid lightgray;}
.photoWraper>div>img{max-width: 48px;max-height: 48px;}
.photoWraper>div:hover>img{max-width: 64px;max-height: 64px;}
.photoWraper .close{top:-5px;right:-10px;position:absolute;z-index: 1;display: none}
.photoWraper:hover .close{display: block}
.photoWraper label{text-align: center;background-color: #f5f5f5; height: 40px; overflow: hidden;width: 100%;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;}
.photoWraper label:hover{background-color: #5bc0de;}
.photoDiv{
    -webkit-box-shadow: 0 5px 15px rgba(0,0,0,.5);
    box-shadow: 0 5px 15px rgba(0,0,0,.5);
    z-index: 2;
    max-width: 90%;
    max-height: 90%;
    background-color:#ccc;
    margin: auto;
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    z-index: 10000;
}
.form-control[disabled]{
    background-color: white;
    border: none;
    box-shadow: none;
}

ul.nav.nav-tabs{ background: #f5f5f5; }

.x_panel.wider{ padding-right: 0;padding-left: 0 }
.x_panel.wider>.x_title{ margin:0 15px; }
.x_panel.wider>.x_content{ z-index: 1;max-height:780px;overflow-y: auto; }
/**
自定义的树
*/
.fengTree {
    padding-left: 0;
}
.fengTree li{
    padding-left:15px;
    display: block;
}
.fengTree i.icon{
    cursor: pointer;
}
.fengTree i.icon:hover{
    color: #008800;
}
.fengTree .arrowDiv{
    display: inline-block;
    width: 14px;
    margin-top: -4px;
    vertical-align: middle;
}
.fengTree .arrowDiv i{
    display: block;
    margin-left: 0;
}
.fengTree .form-inline{
    white-space: nowrap;
    line-height: 32px;
    float:left;
    min-width: 100%;
}
.fengTree .form-inline .text{
    display: inline-block;line-height: 32px;
}
.fengTree .form-inline:hover:not(.bg-danger){
    background-color: #F5F5F5;
}
.fengTree .form-inline .edit-span{
    visibility: hidden;
}
.fengTree .form-inline:hover .edit-span{
    visibility: visible;
}
.fengTree i{
    margin-left: 2px;
    margin-right: 3px;
}
.fengTree span+span{
    margin-left: 15px;
}
.fengTree span.highLight{
    font-weight: bold;
    color: #00f;
}

.layerSelect{
    left: 100%;
    top: 0px;
    z-index: 4;
    background-color: white;
    position:absolute;
}
.layerSelect ul{
    padding-left: 0;
    border: 1px solid rgba(0,0,0,.15);
    border-radius: 4px;
    box-shadow: 0 6px 12px rgba(0,0,0,.175);
    margin-bottom: 0;
}
.layerSelect li{
    padding:0 5px;
    display: block;
    position: relative;
}
.layerSelect li:hover{
    background-color: #F5F5F5;
}
.layerSelect li>.layerSelect{
    display: none;
}
.layerSelect li:hover>.layerSelect{
    display: block;
}
.layerSelect .form-inline{
    white-space: nowrap;
    line-height: 32px;
    float:left;
    min-width: 100px;
}
.layerSelect .form-inline .glyphicon {
    line-height: 32px;
}
.layerSelect .form-inline .glyphicon-menu-right {
    position:absolute ;
    right: 5px;
}
[uib-typeahead-popup].dropdown-menu {
    max-height: 300px;
    overflow-y: auto;
}

.ellipsis{
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.tooltip-inner{
    max-width: 120px;
    word-break: break-all;
}
input.checkbox-8-2[type="checkbox"]{
    display:none
}
input.checkbox-8-2[type="checkbox"]+label {
    cursor: pointer;
    font-size: 1em;
    box-sizing:content-box;
    vertical-align:bottom;
}
[class^="checkbox-8-"]+label {
    padding: 9px;
    border-radius: 25px;
    display: inline-block;
    position: relative;
    width: 20px;
    height: 0px;
    margin-bottom:0;
}

.checkbox-8-2+label:after,.checkbox-8-2+label:before,.checkbox label {
    -webkit-transition: all 0.1s ease-in;
    transition: all 0.1s ease-in;
}

[class^="checkbox-8-"]+label:before {
    content: ' ';
    position: absolute;
    background: white;
    top: 1px;
    left: 1px;
    z-index: 1;
    width: 16px;
    height: 16px;
    border-radius: 100px;
    box-shadow: 0 0 2px rgba(0, 0, 0, 0.5), inset 0 -18px 15px -10px
    rgba(0, 0, 0, 0.05);
}
[class^="checkbox-8-"]:checked+label:before {
    left: 22px;
}

.checkbox-8-2+label:after,.checkbox-8-2+label:before,.checkbox label,.checkbox-8-4+label:after,.checkbox-8-4+label:before,.checkbox label
{
    -webkit-transition: all 0.1s ease-in;
    transition: all 0.1s ease-in;
}

[class^="checkbox-8-"]+label:after {
    content: ' ';
    position: absolute;
    background: #E6332C;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    border-radius: 100px;
    box-shadow: inset 0 0 20px rgba(0, 0, 0, 0.2);
}
[class^="checkbox-8-"]:disabled+label:after {
    background: #aaa;
}

[class^="checkbox-8-"]:checked+label:after {
    background: #63C99C;
}
[class^="checkbox-8-"]:disabled:checked+label:after {
    background: #aaa;
}


